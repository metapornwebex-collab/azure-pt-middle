{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous"],
      "serve": "/index.html"
    },
    {
      "route": "/admin",
      "allowedRoles": ["admin"],
      "redirect": "https://salmon-tree-0bec8db00.2.azurestaticapps.net"
    },
    {
      "route": "/sales",
      "allowedRoles": ["sales"],
      "redirect": "https://witty-hill-046f15c00.1.azurestaticapps.net"
    },
    {
      "route": "/user",
      "allowedRoles": ["user"],
      "redirect": "https://red-field-013434800.1.azurestaticapps.net"
    },
    {
      "route": "/callback",
      "allowedRoles": ["authenticated"],
      "serve": "/callback.html"
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/aad?post_login_redirect_uri=/callback"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/*",
      "serve": "/404.html"
    }
  ]
}
